			},
			"edition": {
				"title": "OS Edition",
				"type": "string"
			},
			"lang": {
				"title": "Language",
				"type": "string",
				"pattern": "^[a-z]{2}$"
			},
			"name": {
				"title": "Name",
				"type": "string"
			},
			"sp_name": {
				"title": "OS Service Pack",
