			"type_id": {
				"enum": [
					99,
					0,
					101,
					100,
					200,
					201,
					300,
					301,
					302,
					400,
					401,
					402
				],
				"title": "Type ID",
				"type": "integer"
			},
			"version": {
				"title": "Version",
				"type": "string"
			}
		},
		"required": [
			"type_id",
			"name"
		],
		"title": "Operating System (OS)",
		"type": "object"
	}
