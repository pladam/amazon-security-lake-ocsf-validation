{
	"ldap_person": {
		"properties": {
			"cost_center": {
				"title": "Cost Center",
				"type": "string"
			},
			"created_time": {
				"title": "Created Time",
				"type": "integer",
				"pattern": "^[0-9]{10,13}$"
			},
			"created_time_dt": {
				"title": "Created Time",
				"type": "string",
				"pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}\\.\\d{6}$"
			},
			"deleted_time": {
				"title": "Deleted Time",
